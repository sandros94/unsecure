{
  "name": "unsecure",
  "version": "0.0.1",
  "description": "Low-level Security utilities",
  "repository": "sandros94/unsecure",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.mts",
      "default": "./dist/utils.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm prepack && changelogen --release --push --publish",
    "test": "pnpm lint && pnpm test:types && vitest run --slowTestThreshold=500",
    "test:coverage": "pnpm lint && pnpm test:types && vitest run --coverage --slowTestThreshold=500",
    "test:types": "tsc --noEmit --skipLibCheck",
    "bench": "vitest bench"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.2.4",
    "automd": "^0.4.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.29.0",
    "eslint-config-unjs": "^0.4.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.1"
}
