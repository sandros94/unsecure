{
  "name": "unsecure",
  "version": "0.0.6",
  "description": "Low-level Security utilities",
  "repository": "sandros94/unsecure",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    "./package.json": "./package.json",
    "./utils": "./dist/utils.mjs",
    ".": {
      "types": "./dist/index.d.mts",
      "utils": "./dist/utils.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "obuild",
    "dev": "vitest dev",
    "lint": "eslint --cache . && prettier -c src test",
    "lint:fix": "automd && eslint --cache . --fix && prettier -c src test -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm prepack && changelogen --release --push",
    "test": "pnpm lint && pnpm test:types && vitest run --slowTestThreshold=500",
    "test:coverage": "pnpm lint && pnpm test:types && vitest run --coverage --slowTestThreshold=500",
    "test:types": "tsc --noEmit --skipLibCheck",
    "bench": "vitest bench --run"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.2.4",
    "automd": "^0.4.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.34.0",
    "eslint-config-unjs": "^0.5.0",
    "obuild": "^0.4.20",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "22"
  },
  "packageManager": "pnpm@10.28.1"
}
